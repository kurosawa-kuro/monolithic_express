<table class="table table-bordered">
  <thead class="thead-dark">
    <tr>
      <th scope="col">#</th>
      <th scope="col">First Name</th>
      <th scope="col">Last Name</th>
      <th scope="col">Email</th>
      <th scope="col">Phone</th>
      <th scope="col" class="text-end">Action</th>
    </tr>
  </thead>
  <tbody>

    <% rows.forEach(function (value, key) { %>
      <tr>
        <th scope="row"><%= value.id %></th>
        <td><%= value.name %></td>
        <td><%= value.name %></td>
        <td><%= value.name %></td>
        <td><%= value.name %></td>
        <td class="text-end">
          <a href="/viewuser/<%= value.id %>" type="button" class="btn btn-light btn-small"><i class="bi bi-eye"></i> View</a>
          <a href="/edituser/<%= value.id %>" type="button" class="btn btn-light btn-small"><i class="bi bi-pencil"></i>
            Edit</a>
          <a href="/<%= value.id %>" type="button" class="btn btn-light btn-small"><i class="bi bi-person-x"></i> Delete</a>
        </td>
      </tr>
    <% }); %>

  </tbody>
</table>